{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "width": "container",
  "height": "container",
  "data": {
    "values": []
  },
  "config": {
    "customFormatTypes": true
  },
  "mark": "area",
  "encoding": {
    "x": {
      "field": "date",
      "title": "Date",
      "type": "nominal",
      "axis": {
        "labelAngle": 45,
        "title": null
      }
    },
    "y": {
      "title": "Sum of Ratio",
      "aggregate": "sum",
      "field": "ratio",
      "type": "quantitative",
      "axis": {
        "labelAngle": 45
      }
    },
    "color": {
      "field": "tagId",
      "type": "nominal",
      "scale": {
        "scheme": "category20b"
      },
      "legend": {
        "orient": "top",
        "format": "number",
        "formatType": "customFormatter",
        "labelFontSize": 14
      }
    }
  }
}
