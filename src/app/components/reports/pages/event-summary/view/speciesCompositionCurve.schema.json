{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "width": "container",
  "height": "container",
  "data": {
    "values": []
  },
  "mark": "area",
  "encoding": {
    "x": {
      "field": "date",
      "type": "nominal",
      "axis": {
        "labelAngle": 0
      }
    },
    "y": {
      "aggregate": "sum",
      "field": "ratio",
      "type": "quantitative",
      "axis": {
        "labelAngle": 0
      }
    },
    "color": {
      "field": "tagId",
      "type": "nominal",
      "scale": {
        "scheme": "category20b"
      }
    }
  }
}
