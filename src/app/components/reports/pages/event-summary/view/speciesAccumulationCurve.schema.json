{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "width": "container",
  "height": "container",
  "data": {
    "values": []
  },
  "layer": [
    {
      "mark": {
        "type": "point",
        "filled": true,
        "color": "black"
      },
      "encoding": {
        "x": {
          "title": "Date",
          "field": "date",
          "type": "nominal",
          "axis": {
            "labelAngle": 45
          }
        },
        "y": {
          "title": "Count",
          "field": "count",
          "type": "quantitative",
          "axis": {
            "labelAngle": 0
          }
        }
      }
    },
    {
      "mark": {
        "type": "errorband",
        "extent": "ci",
        "interpolate": "monotone",
        "color": "black",
        "opacity": 0.1,
        "tension": 0.5
      },
      "encoding": {
        "x": {
          "field": "date",
          "type": "nominal"
        },
        "y": {
          "title": "Count",
          "field": "count",
          "type": "quantitative"
        },
        "yError": {
          "field": "error"
        }
      }
    },
    {
      "mark": {
        "type": "line",
        "interpolate": "monotone"
      },
      "encoding": {
        "x": {
          "field": "date",
          "type": "nominal",
          "axis": {
            "labelAngle": 45
          }
        },
        "y": {
          "title": "Count",
          "field": "count",
          "type": "quantitative",
          "axis": {
            "labelAngle": 0
          }
        }
      }
    }
  ]
}
