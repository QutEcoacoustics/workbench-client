{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "width": "container",
  "height": "container",
  "data": {
    "values": []
  },
  "layer": [
    {
      "mark": {
        "type": "point",
        "filled": true,
        "color": "black"
      },
      "encoding": {
        "x": {
          "field": "date",
          "type": "nominal",
          "axis": { "labelAngle": 0 }
        },
        "y": {
          "field": "count",
          "type": "quantitative",
          "axis": { "labelAngle": 0 }
        }
      }
    },
    {
      "mark": {
        "type": "errorbar",
        "extent": "ci",
        "ticks": true,
        "color": "black"
      },
      "encoding": {
        "x": {
          "field": "date",
          "type": "nominal"
        },
        "y": {"field": "count", "type": "quantitative"},
        "yError": { "field": "error" }
      }
    },
    {
      "mark": "line",
      "encoding": {
        "x": {
          "field": "date",
          "type": "nominal",
          "axis": { "labelAngle": 0 }
        },
        "y": {
          "field": "count",
          "type": "quantitative",
          "axis": { "labelAngle": 0 }
        }
      }
    }
  ]
}
