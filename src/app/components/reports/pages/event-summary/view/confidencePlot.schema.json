{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "width": "container",
  "height": "container",
  "data": {
    "values": []
  },
  "mark": {
    "type": "area",
    "interpolate": "monotone"
  },
  "encoding": {
    "x": {
      "field": "data",
      "type": "quantitative",
      "axis": null
    },
    "y": {
      "aggregate": "sum",
      "field": "data",
      "axis": null,
      "stack": "center"
    },
    "color": {
      "field": "category",
      "type": "nominal",
      "scale": {
        "scheme": "category20"
      },
      "legend": null
    }
  },
  "config": {
    "view": {
      "stroke": "transparent"
    }
  }
}
