{
  "name": "bawclient",
  "author": {
    "name": "Charles Alleman",
    "email": "charles@alleman.com.au"
  },
  "description": "Client-side website for online ecoacoustic analysis.",
  "version": "3.0.0",
  "scripts": {
    "build": "nx build --configuration production",
    "e2e": "nx e2e",
    "e2e:debug": "node --max_old_space_size=4096 --inspect-brk ./node_modules/protractor/bin/protractor e2e/protractor.conf.js",
    "lint": "node --max_old_space_size=4096 ./node_modules/eslint/bin/eslint.js .",
    "ng": "node --max_old_space_size=4096 ./node_modules/@angular/cli/bin/ng",
    "nx": "nx",
    "start": "nx serve --host 0.0.0.0 --disableHostCheck",
    "stats": "nx build --configuration development --stats-json && webpack-bundle-analyzer dist/workbench-client/browser/stats.json",
    "test": "npm run test:chrome",
    "test:chrome": "nx test -- --code-coverage --browsers Chrome",
    "test:firefox": "nx test -- --code-coverage --browsers Firefox",
    "test:edge": "nx test -- --code-coverage --browsers Edge",
    "test:all": "nx test -- --code-coverage --browsers Firefox,Chrome,Edge",
    "test:pipeline": "nx test -- --watch=false --code-coverage",
    "dev:ssr": "nx run workbench-client:serve-ssr",
    "serve:ssr": "node dist/workbench-client/server/main.js",
    "build:ssr": "npm run build && nx run workbench-client:server:production",
    "prerender": "nx run workbench-client:prerender",
    "postinstall": "node --max_old_space_size=4096 ./decorate-angular-cli.js",
    "webdriver": "node ./node_modules/protractor/node_modules/webdriver-manager/bin/webdriver-manager"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^12.2.4",
    "@angular/cdk": "^12.2.4",
    "@angular/common": "^12.2.3",
    "@angular/compiler": "^12.2.1",
    "@angular/core": "^12.2.3",
    "@angular/forms": "^12.2.4",
    "@angular/google-maps": "^12.2.3",
    "@angular/localize": "^12.2.4",
    "@angular/platform-browser": "^12.2.3",
    "@angular/platform-browser-dynamic": "^12.2.4",
    "@angular/platform-server": "^12.2.3",
    "@angular/router": "^12.2.4",
    "@fortawesome/angular-fontawesome": "^0.9.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.36",
    "@fortawesome/free-solid-svg-icons": "^5.15.4",
    "@ng-bootstrap/ng-bootstrap": "^10.0.0",
    "@nguniversal/express-engine": "^12.1.0",
    "@ngx-formly/bootstrap": "^5.10.22",
    "@ngx-formly/core": "^5.10.22",
    "@ngx-loading-bar/core": "^5.1.2",
    "@ngx-loading-bar/http-client": "^5.1.2",
    "@ngx-ssr/cache": "^1.1.1",
    "@ngx-ssr/timeout": "^1.0.2",
    "@swimlane/ngx-datatable": "^19.0.0",
    "@vvo/tzdb": "^6.10.0",
    "bootstrap": "^5.1.0",
    "d3-color": "^3.0.1",
    "express": "^4.17.1",
    "filesize": "^8.0.0",
    "full-icu": "^1.3.4",
    "http-status": "^1.5.0",
    "humanize-duration": "^3.27.0",
    "immutable": "^4.0.0-rc.14",
    "include-media": "^1.4.10",
    "just-camel-case": "^4.0.2",
    "just-snake-case": "^1.2.1",
    "luxon": "^2.0.2",
    "ngx-captcha": "^10.0.0",
    "ngx-cookie-service": "^12.0.3",
    "ngx-device-detector": "^2.1.1",
    "ngx-toastr": "^14.1.0",
    "rxjs": "^6.6.7",
    "tslib": "^2.3.1",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^12.2.4",
    "@angular-eslint/builder": "12.3.1",
    "@angular-eslint/eslint-plugin": "12.3.1",
    "@angular-eslint/eslint-plugin-template": "^12.3.1",
    "@angular-eslint/schematics": "12.3.1",
    "@angular-eslint/template-parser": "12.3.1",
    "@angular/cli": "^12.2.2",
    "@angular/compiler-cli": "^12.2.3",
    "@angular/language-service": "^12.2.4",
    "@chiragrupani/karma-chromium-edge-launcher": "^2.1.1",
    "@ngneat/spectator": "^8.0.4",
    "@nguniversal/builders": "^12.1.0",
    "@nrwl/nx-cloud": "12.3.10",
    "@nrwl/workspace": "12.8.0",
    "@types/d3-color": "^3.0.2",
    "@types/express": "^4.17.13",
    "@types/faker": "^5.5.8",
